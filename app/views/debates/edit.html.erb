<% provide(:title, 'Edit Debate') %>
<h1>Edit Debate</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@debate) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>
      <%= render 'debate_form', :f => f  %>
		    <%= f.submit "Save changes", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>