<div class='pix12'>
  <% if user_signed_in? %>
    <% if current_user.voted_for? point %>
      <%= link_to "Unupvote", unimportant_point_path(point), method: :put %> 
    <%else%>
      <%= link_to "Upvote", important_point_path(point), method: :put %>   
    <%end%> 
  <%else%>
    <%= link_to "Upvote", important_point_path(point), method: :put %> 
  <%end%>         
</div>
<%=point.votes_for.size%>