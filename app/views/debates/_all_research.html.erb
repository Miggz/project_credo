<p class = 'pix12'>
  <%= link_to "View less research("+point.findings.count('research_id', distinct:true).to_s+")", less_research_debates_path(point: point), remote: true%> | 
  <%= link_to "Edit", edit_point_path(point) %> |
  <%= link_to "Add findings", findings_path(point: point) %> 
</p>  
<ul class= 'research'>	
	<% findings = point.findings.sort! {|a,b| b.research.score <=> a.research.score}%>
	<% findings.each do |finding| %>
		<%= render 'debates/findings', finding: finding %>
	<%end%> 
</ul>	
<%= link_to "View less research("+point.findings.count('research_id', distinct:true).to_s+")", less_research_debates_path(point: point), remote: true%>